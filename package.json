{
  "name": "metanet-bank-gateway-api",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "build:dev": "rimraf dist/ && tsc -p tsconfig.json && yarn copy-assets",
    "build": "rimraf dist/ && webpack && yarn copy-assets",
    "lint-fix": "eslint . --ext .ts --fix",
    "check": "tsc -p tsconfig.json --noEmit",
    "copy-assets": "copyfiles -u 1 ./src/assets/**/* ./dist/",
    "migrate": "npx knex migrate:latest",
    "migrate:up": "npx knex migrate:up",
    "migrate:down": "npx knex migrate:down",
    "migrate:rollback": "npx knex migrate:rollback",
    "migrate:test": "npx knex migrate:latest --env test",
    "migrate-rollback:test": "npx knex migrate:rollback --env test",
    "seed": "npx knex seed:run",
    "seed:test": "npx knex seed:run --env test",
    "dev": "run-script-os",
    "dev:win32": "ts-node -r tsconfig-paths/register src/index.ts | pino-pretty -c -l",
    "dev:darwin:linux": "nodemon",
    "dev:default": "nodemon",
    "start": "pm2-runtime dist/index.js",
    "sandbox": "ts-node -r tsconfig-paths/register src/sandbox.ts",
    "cronJob:dev": "ts-node -r tsconfig-paths/register src/command/cronJobs.ts",
    "cronJob": "pm2-runtime dist/src/command/cronJobs.js",
    "test": "jest --forceExit --runInBand --detectOpenHandles",
    "test:watch": "jest --watch --onlyChanged",
    "test:clearCache": "jest --clearCache --forceExit --runInBand --detectOpenHandles",
    "test:dev": "jest --watchAll --detectOpenHandles",
    "eslint": "eslint --fix --ext .ts,.js .",
    "lint": "npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Metanet-Corporation/minecraft-topup-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/Metanet-Corporation/minecraft-topup-api#readme",
  "dependencies": {
    "@sentry/node": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "@types/randomatic": "^3.1.3",
    "ajv": "^6.12.6",
    "ajv-errors": "^1.0.1",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "commander": "^2.15.1",
    "crypto": "^1.0.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^8.0.1",
    "fast-xml-parser": "^3.19.0",
    "fastify": "^3.17.0",
    "fastify-cookie": "5.5.0",
    "fastify-cors": "^6.0.1",
    "fastify-helmet": "^5.3.2",
    "fastify-multipart": "^4.0.7",
    "fastify-rate-limit": "^5.6.2",
    "fastify-swagger": "^4.7.0",
    "husky": "^7.0.4",
    "joi": "^17.8.3",
    "knex": "^0.95.7",
    "luxon": "^3.3.0",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.2",
    "pino": "^7.9.2",
    "pm2": "^5.1.1",
    "promptparse": "^0.5.0",
    "run-script-os": "^1.1.6",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.10"
  },
  "devDependencies": {
    "@swc/core": "^1.3.44",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.5.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^15.14.9",
    "@types/pino": "^7.0.5",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.49.0",
    "eslint-config-maqe": "^4.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^7.6.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
